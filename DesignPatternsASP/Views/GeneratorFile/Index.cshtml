<div class="row">
	<div clas="col-8 offset-2">
		<div class="form-group">
			<select id="optionFile" class="form-control">
				<option selected value="" disabled> --Selecciona una opcion--</option>
				<option value="1">Json</option>
				<option value="2">Pipes</option>
			</select>
		</div>
		<div class="form-group">
			<button class="btn btn-success" onclick="generate();">Generar</button>
		</div>
	</div>
</div>

<script>
	function generate() {
		let optionFile = document.getElementById("optionFile").value;
		fetch("@Url.Content("~/GeneratorFile/CreateFile")" + "?optionFile="+optionFile)
			.then(resp => resp.json())
			.then(json => alert(json));
	}
</script>
